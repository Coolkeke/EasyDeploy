<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.MergedDictionaries>
        <!--定制原生控件-->
        <ResourceDictionary Source="/EasyDeploy;component/Themes/SvgButtonStyle.xaml"/>
        <ResourceDictionary Source="/EasyDeploy;component/Themes/IceButtonStyle.xaml"/>
        <ResourceDictionary Source="/EasyDeploy;component/Themes/IceDataGridStyle.xaml"/>
        <ResourceDictionary Source="/EasyDeploy;component/Themes/IceRichTextBoxStyle.xaml"/>
        <ResourceDictionary Source="/EasyDeploy;component/Themes/IceTabControlStyle.xaml"/>
        <ResourceDictionary Source="/EasyDeploy;component/Themes/IceTabItemStyle.xaml"/>
        <!--设置语言-->
        <ResourceDictionary Source="/EasyDeploy;component/Language/en-US.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    <!--通用默认样式-->
    <Style TargetType="{x:Type ScrollViewer}" BasedOn="{StaticResource ScrollViewerStyle}" />
</ResourceDictionary>